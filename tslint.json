{
	"extends": "dtslint/dtslint.json",
	"linterOptions": {
		"exclude": [
			"api_params/index.d.ts"
		]
	},
	"rules": {
		"semicolon": false,
		"indent": [true, "spaces"],
		"no-padding": false,
		"no-unnecessary-qualifier": false,
		"unified-signatures": false,
        "no-redundant-jsdoc": false
	}
}
